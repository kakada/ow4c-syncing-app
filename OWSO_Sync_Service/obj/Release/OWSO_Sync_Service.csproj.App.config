<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0" />
  </startup>
  <appSettings>
    <add key="base_url" value="https://owso-stg.ilabsea.org/api/v1/" />
    <add key="database_sync_api" value="sites/{0}" />
    <add key="health_status_update_api" value="me?sync_status={0}&amp;site_code={1}" />
    <add key="connection_time_out" value="15" />
    <!-- unit in seconds -->
    <add key="site_code" value="0202" />
    <add key="interval" value="1" />
    <!-- interval in minute -->
    <add key="database_url" value="Server=localhost;Database=owso_db;User ID=chandara;Password=12345678" />
    <!--  ticketID is a combination among 3 fields(DistGIS, RequestedDate(yymm), and FormID(4 digits format) -->
    <add key="query" value="SELECT CONCAT([Services].DistGIS, FORMAT ([Services].RequestedDate, 'yyMM'), FORMAT ([Services].FormID, '0000')) AS ticketID, [Services].*, [tbl-delivery].ApprovedStatus, [tbl-delivery].ApprovedDate FROM [Services] INNER JOIN [tbl-delivery] ON [Services].[ID] = [tbl-delivery].[ServiceID] WHERE CONVERT(int, [Services].timestamp)&gt;{0}" />
    <add key="query_max_timestamp" value="SELECT MAX(CONVERT(int, timestamp)) FROM [owso_db].[dbo].[services]" />
    <!--<add key="compare_date_time_format" value="yyyy-MM-dd H:mm:ss" />-->
    <add key="access_token" value="m2V93YiZrNAakVMV" />
    <add key="ClientSettingsProvider.ServiceUri" value="" />
  </appSettings>
